<div class="ingblock">
<div class="ing-container">
<h1>Recipes Has Ingredients</h1>
<h1></h1>
<div align="center">
<table>
  <thead>
    <tr>
      <th>Recipe</th>
      <th>Ingredient</th>
      <th>Quantity</th>

    
      <th colspan="3"></th>
    </tr>
  </thead>
</div>
  <tbody>
    <% @recipes_has_ingredients.each do |recipes_has_ingredient| %>
      <tr>
        <td><%= recipes_has_ingredient.recipe.name %></td>
        <td><%= recipes_has_ingredient.ingredient.name %></td>
        <td><%= recipes_has_ingredient.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Recipes Has Ingredient', new_recipes_has_ingredient_path %>