<style>
  .quik td{
    background: none;
}
  .quik th{
    background: none;
}
</style>

<div class="ingblock1">


    <% @quik_recipes.each do |recipe| %>
    <table class= "quik">


      <tr>
        <th><h3><%= recipe.name %> <%= recipe.totaltime %> min</h3></th>
      </tr>
      

    <tr>
      <td><iframe width="300" height="200" src="<%= recipe.videourl %>" frameborder="0" allowfullscreen></iframe></td>
    </tr>
    <% end %>
</table>

</div>

